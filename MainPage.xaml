<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:blazor="clr-namespace:Microsoft.AspNetCore.Components.WebView.Maui;assembly=Microsoft.AspNetCore.Components.WebView.Maui"
             xmlns:local="clr-namespace:MyMauiApp"
             xmlns:components="clr-namespace:MyMauiApp.Components"
             x:Class="MyMauiApp.MainPage">

    <blazor:BlazorWebView HostPage="wwwroot/index.html">
        <blazor:BlazorWebView.RootComponents>
            <blazor:RootComponent
                Selector="#app"
                ComponentType="{x:Type components:Routes}"/>
        </blazor:BlazorWebView.RootComponents>
    </blazor:BlazorWebView>

</ContentPage>
